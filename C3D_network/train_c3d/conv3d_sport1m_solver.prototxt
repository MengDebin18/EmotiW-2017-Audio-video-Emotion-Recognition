train_net: "conv3d_sport1m_train.prototxt"
test_net: "conv3d_sport1m_test.prototxt"
test_iter: 50
test_interval: 100
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.005
lr_policy: "step"
gamma: 0.1
stepsize: 800
# Display every 20 iterations
display: 20
# The maximum number of iterations
max_iter: 2000
# snapshot intermediate results
snapshot: 200
snapshot_prefix: "model/conv3d_sport1m_1_2_171_0001"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 3
